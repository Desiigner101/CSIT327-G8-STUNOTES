{
  "version": 2,
  "builds": [
    {
      "src": "stunotesapp/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.12" 
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static_cdn/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "stunotesapp/wsgi.py"
    }
  ],
  "outputDirectory": "static_cdn"
}